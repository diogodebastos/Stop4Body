      "legPos":"top",
      "min":200.0,
      "max":1000.0,
      "bins":40
    },
    {
     "expression":"Jet1Pt",
     "label":"p_{T}(jet 1) [GeV]",
     "legPos":"top",
      "min":100.0,
      "max":1000.0,
      "bins":50
