{
 "cuts":[
   {
     "name":"PS_e_channel",
     "expression":"(DPhiJet1Jet2 < 2.5 || Jet2Pt < 60) && (HT > 200) && (Met > 280) && (Jet1Pt > 110) && (nGoodEl_cutId_loose == 1) && (nGoodMu == 0)",
     "latex":"Preselection ($e$ channel)"
   }
  ],
  "variables":[
    {
      "expression":"HT",
